<!-- Modal Dialog -->
<div class="modal fade" tabindex="-1" role="dialog" id="myModal">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Edición de formularios</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <table>
          <tr>
            <th>Id:</th>
            <td><input type="text" class="form-control" placeholder="Número de formulario" name="formId" readonly="readonly"></td>
          </tr><tr>
            <th>Título:</th>
            <td><input type="text" class="form-control" placeholder="Título del formulario" name="titulo"></td>
          </tr><tr>
            <th>Desde:</th>
            <td><input type="text" class="form-control" placeholder="fecha de inicio en formato AAAA-MM-DD" name="desde"></td>
          </tr><tr>
            <th>Hasta:</th>
            <td><input type="text" class="form-control" placeholder="fecha de fin (inclusive) en formato AAAA-MM-DD" name="hasta"></td>
          </tr><tr>
            <th>Público:</th>
            <td>
              <label><input type="checkbox" name="publico"> Acceso sin pasar por el Portal de Servicios</label>
            </td>
          </tr><tr>
            <th>Múltiple:</th>
            <td>
              <label><input type="checkbox" name="multiple"> Aceptar más de una respuesta por usuario</label>
            </td>
          </tr>
        </table>
        <div id="message" class="alert alert-danger"></div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
        <button type="button" class="btn btn-primary" id="modalSubmit">Enviar</button>
      </div>
    </div>
  </div>
</div>
