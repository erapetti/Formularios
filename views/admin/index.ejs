<div id="admin">

  <h1> <%= config.titulo %> </h1>

  <form method="post" id="adminForm">

    <div class="panel panel-default content">
      <div class="panel-body">
        <table class="table formularios">
          <tr>
            <th>Id</th>
            <th>TÃ­tulo</th>
            <th>Desde</th>
            <th>Hasta</th>
            <th>Recibidos</th>
            <th>Acciones</th>
          </tr>
          <% config.forEach(function(item){ %>
            <tr>
              <td>
                <%= item.formid %>
              </td><td>
                <%= item.titulo %>
              </td><td>
                <%= item.desde.toString() %>
                </td><td>
                  <%= item.hasta.toString() %>
                </td><td style="text-align:center">
                  <span class="badge">
                    <%= cantidad[item.formid]>0 ? cantidad[item.formid] : 0 %>
                  </span>
                </td>
                <td style="text-align:center">
                  <a href="/admin/download?id=<%= item.formid %>"<% if (!(cantidad[item.formid]>0)) { %> class="disabled"<% } %>><span class="glyphicon glyphicon-download-alt" aria-hidden="true"></span></a>
                  &nbsp;
                  <a href="#"<% if (cantidad[item.formid]>0) { %> class="disabled"<% } %>><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a>
                  &nbsp;
                  <a href="#"><span class="glyphicon glyphicon-duplicate" aria-hidden="true"></span></a>
                  &nbsp;
                  <a href="#"<% if (cantidad[item.formid]>0) { %> class="disabled"<% } %>><span class="glyphicon glyphicon-remove-sign" aria-hidden="true"></span></a>
                <td>
              </tr>
          <% }) %>
        </table>
      </div>
    </div>

  </form>

</div>
