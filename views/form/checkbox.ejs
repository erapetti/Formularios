<tr class="modulo" data-orden="<%= m.orden %>">
  <td class="col-sm-3">
    <label for="checkbox-<%= m.orden%>"><%- m.etiqueta %>:</label>
  </td><td class="col-sm-6">
    <div class="form-check">
      <input type="checkbox" class="form-check-input" id="checkbox-<%= m.orden%>" name="<%= m.nombre %>" <% if (m.value) { %>checked="checked"<% } %><% if (m.validador) { %> data-validate="<%= m.validador %>"<% } %><% if (m.opcional) { %> data-optional="true"<% } %>>
      <label class="form-check-label" for="checkbox-<%= m.orden%>">
        <% if (m.ayuda !== null) { %><%= m.ayuda %><% } %>
      </label>
    </div>
  </td><td class="col-sm-3">
  </td>
</tr>
<% if (m.texto1) { %>
  <tr>
    <td colspan="3" style="border-top:none"><%- m.texto1 %></td>
  </tr>
<% } %>
