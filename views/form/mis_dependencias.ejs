<tr class="modulo" data-orden="<%= m.orden %>">
  <td class="col-sm-3">
    <label for="campo-<%= m.id%>"><%- m.etiqueta %>:</label>
  </td><td class="col-sm-6">
    <select class="custom-select" id="campo-<%= m.id%>" name="<%= m.nombre %>"<% if (m.validador) { %> data-validate="<%= m.validador %>"<% } %><% if (m.opcional) { %> data-optional="true"<% } %>>
      <option value="">Seleccione una opci√≥n</option>
      <% m.options.forEach(function(dependencia){ -%>
        <option value="<%= dependencia.DependDesc %> (<%= dependencia.DependId %>)"<% if (m.value === dependencia.DependDesc+" ("+dependencia.DependId+")") { %> selected<% } %>><%= dependencia.DependDesc %></option>
      <% }) %>
    </select>
  </td><td class="col-sm-3">
    <% if (m.ayuda !== null) { %><%= m.ayuda %><% } %>
  </td>
</tr>
